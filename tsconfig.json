{
  "include": [
    "./src/**/*.ts"
  ],
  "compilerOptions": {
    "incremental": true,
    "allowJs": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": true,
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  }
}